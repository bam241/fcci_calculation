<simulation xmlns:xi="http://www.w3.org/2001/XInclude">

    <schematype>flat</schematype>
    <control>
        <!--<duration>1800</duration>-->
        <duration>2400</duration>
        <startmonth>1</startmonth>
        <startyear>2015</startyear>
        <decay>never</decay>
        <explicit_inventory>true</explicit_inventory>
    </control>

    <archetypes>
        <spec> <lib>cycamore</lib> <name>Source</name></spec>
        <spec> <lib>cycamore</lib> <name>GroupInst</name></spec>
        <spec> <lib>cycamore</lib> <name>DeployInst</name></spec>
        <spec> <lib>cycamore</lib> <name>Sink</name></spec>
        <spec> <lib>cycamore</lib> <name>Enrichment</name> </spec>
        <spec> <lib>cycamore</lib> <name>Reactor</name> </spec>
        <spec> <lib>cycamore</lib> <name>Storage</name> </spec>
        <spec> <lib>cycamore</lib> <name>FuelFab</name> </spec>
        <spec> <lib>cycamore</lib> <name>Separations</name> </spec>
        <spec> <lib>cycamore</lib> <name>Mixer</name> </spec>
    </archetypes>


  <recipe>
    <name>r_u_nat</name>
    <basis>mass</basis>
    <nuclide><id>U234</id>   <comp>7.11000E-03</comp> </nuclide>
    <nuclide><id>U235</id>   <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>U236</id>   <comp>9.92890E-01</comp> </nuclide>
    <nuclide><id>U238</id>   <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>NP237</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>PU236</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>PU238</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>PU239</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>PU240</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>PU241</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>PU242</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>AM241</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>AM242M</id> <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>AM243</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>CM242</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>CM243</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>CM244</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>CM245</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>CM246</id>  <comp>0.00000E+00</comp> </nuclide>
    <nuclide><id>CS237</id>  <comp>0.00000E+00</comp> </nuclide>
  </recipe>

    <prototype>
        <name>nu_src</name>
        <config>
            <Source>
                <outcommod>nu</outcommod>
                <outrecipe>nu</outrecipe>

                <inventory_size>1e100</inventory_size>
                <throughput>1e100</throughput>
            </Source>
        </config>
    </prototype>


    <prototype>
        <name>waste</name>
        <config>
            <Sink>
                <in_commods><val>waste</val></in_commods>
                <capacity>1e100</capacity>
            </Sink>
        </config>
    </prototype>

    <facility>
        <name>enrichment</name>
        <config>
            <Enrichment>
                <feed_commod>c_u_nat</feed_commod>
                <feed_recipe>r_u_nat</feed_recipe> 
                <product_commod>c_uox</product_commod>
                <tails_commod>waste</tails_commod>
                <max_feed_inventory>1.e100</max_feed_inventory>
                <tails_assay>0.003</tails_assay>
            </Enrichment>
        </config>
    </facility>

    <prototype>
        <name>PWR</name>
        <lifetime>720</lifetime>
        <config>
            <cyclassReactor>
                <eq_model>MLP_Kinf</eq_model>
                <eq_command>/Users/mouginot/work/app/CLASS/DATA_BASES/PWR/UOX/EQModel/MLP_KINF/PWR_UOX.xml,/Users/mouginot/work/app/CLASS/DATA_BASES/PWR/UOX/EQModel/MLP_KINF/PWR_UOX.nfo,4,1.034</eq_command>

                <ir_model>RK4</ir_model>
                <ir_command>none</ir_command>

                <xs_model>XSM_MLP</xs_model>
                <xs_command>/Users/mouginot/work/app/CLASS/DATA_BASES/PWR/UOX/XSModel/30Wg_FullUOX,Data_Base_Info.nfo,false</xs_command>

                <fuel_incommods>
                    <val>c_uox</val>
                </fuel_incommods>
                <fuel_prefs>
                    <val>10</val>
                </fuel_prefs>

                <fuel_outcommods> 
                    <val>c_uox_spent</val>
                </fuel_outcommods>
                <cycle_time>50</cycle_time> <!-- BU/30days *Mcore/batch / P_eff = 50./(30)*88758./3./(3000*0.9) -->
                <refuel_time>0</refuel_time>
                <assem_size>88758</assem_size><!-- Core_inv / Core_Ass = 88758 / 3. -->
                <n_assem_core>1</n_assem_core>
                <n_assem_batch>1</n_assem_batch>
                <power_cap>1000</power_cap> <!-- P_th * Ef_th * Cap_fac = 3000*33.333%*0.9 -->
                <power>3000</power>
                <burnup>50</burnup>

            </cyclassReactor>
        </config>
    </prototype>

    <prototype>
        <name>storage_uox_spent</name>
        <config>
            <Storage>
                <in_commods>
                    <val>c_uox_spent</val>
                </in_commods>
                <out_commods>
                    <val>c_uox_spent_stored</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>

    <prototype>
        <name>deployer_main_CHAIN</name>
        <config>
            <DeployInst>
                <prototypes>
                    <val>waste</val>
                    <val>nu_src</val>
                    <val>leu_src</val>

                </prototypes>

                <build_times>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                </build_times>

                <n_build>
                    <val>1</val>
                    <val>1</val>
                    <val>1</val>
                </n_build>
            </DeployInst>
        </config>
    </prototype>




    <agent> <name>deployer_1</name> <prototype>deployer_main_CHAIN</prototype> </agent>


</simulation>
